{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "env": {
    "PRISMA_GENERATE_SCRIPT": "prisma generate"
  },
  "functions": {
    "postBuild": {
      "runtime": "@vercel/node",
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": [
        "prisma/migrations/**"
      ]
    }
  }
}